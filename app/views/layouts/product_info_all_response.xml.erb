                  </dt_assoc>
                </item>
                <item key='organization'>
                  <dt_assoc>
                    <item key='first_name'><%= @data[:contact_set][:organization][:first_name] %></item>
                    <item key='last_name'><%= @data[:contact_set][:organization][:last_name] %></item>
                    <item key='title'><%= @data[:contact_set][:organization][:title] %></item>
                    <item key='org_name'><%= @data[:contact_set][:organization][:org_name] %></item>
                    <item key='address1'><%= @data[:contact_set][:organization][:address1] %></item>
                    <item key='address2'><%= @data[:contact_set][:organization][:address2] %></item>
                    <item key='city'><%= @data[:contact_set][:organization][:city] %></item>
                    <item key='region'><%= @data[:contact_set][:organization][:region] %></item>
                    <item key='country'><%= @data[:contact_set][:organization][:country] %></item>
