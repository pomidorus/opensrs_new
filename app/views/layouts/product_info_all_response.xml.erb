                    <item key='address2'><%= @data[:contact_set][:tech][:address2] %></item>
                    <item key='city'><%= @data[:contact_set][:tech][:city] %></item>
                    <item key='region'><%= @data[:contact_set][:tech][:region] %></item>
                    <item key='country'><%= @data[:contact_set][:tech][:country] %></item>
                    <item key='country_id'><%= @data[:contact_set][:tech][:country_id] %></item>
                    <item key='postal_code'><%= @data[:contact_set][:tech][:postal_code] %></item>
