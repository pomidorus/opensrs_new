                    <item key='email'><%= @data[:contact_set][:billing][:email] %></item>
                    <item key='fax'><%= @data[:contact_set][:billing][:fax] %></item>
                  </dt_assoc>
                </item>
              </dt_assoc>
            </item>
            <item key='csr'><%= @data[:csr] %></item>
            <item key='upgrade_options'><%= @data[:upgrade_options] %></item>
            <item key='start_date'><%= @data[:start_date] %></item>
          </dt_assoc>
        </item>
      </dt_assoc>
    </data_block>
  </body>
</OPS_envelope>