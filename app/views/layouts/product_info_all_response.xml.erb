                    <item key='postal_code'><%= @data[:contact_set][:organization][:postal_code] %></item>
                    <item key='phone'><%= @data[:contact_set][:organization][:phone] %></item>
                    <item key='email'><%= @data[:contact_set][:organization][:email] %></item>
                    <item key='fax'><%= @data[:contact_set][:organization][:fax] %></item>
                  </dt_assoc>
                </item>
                <item key='billing'>
                  <dt_assoc>
                    <item key='first_name'><%= @data[:contact_set][:billing][:first_name] %></item>
                    <item key='last_name'><%= @data[:contact_set][:billing][:last_name] %></item>
                    <item key='title'><%= @data[:contact_set][:billing][:title] %></item>
                    <item key='org_name'><%= @data[:contact_set][:billing][:org_name] %></item>
                    <item key='address1'><%= @data[:contact_set][:billing][:address1] %></item>
